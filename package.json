  "scripts": {
    "start": "node App.js",
    "dev": "concurrently \"npm run dev-f\" \"nodemon --trace-deprecation App.js\" ",
    "dev-f": "vite --host",
    "dev-s": "nodemon --trace-deprecation App.js",
